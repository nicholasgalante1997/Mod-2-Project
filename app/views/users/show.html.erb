<h1><%= @user.username %></h1>

<p>Reservations: </p> 
<% @user.reservations.each do |r| %>
    <p><%= link_to r.restaurant.name %></p>
    <p><%= r.date %></p>
    <p><%= r.time %> PM </p>
    <p>Party of <%= r.party %></p>
<%end%>

<p>Friends: </p>
<% @user.followers.each do |follower|%>
<p> <%= link_to follower.username, user_path(follower) %></p>
<%end%>

<%= link_to "User Home", users_path %>